#include <stdio.h>
#include <unistd.h>
#include <stdlib.h>
/********FUNÇÕES***********/
int escolha; 
int balao = 0; //encontro com a entidade que se assemelha a um balão//variavel para o final morrendo pro balão
int medo = 0; //encontro com o homem sem rosto
int vida = 1; //1 = vivo, 0 = morto
int menina = 0; //encontro com a criança//variavel para o final morrendo pra menina
int lobo = 0;//variavel para o final morrendo pro lobo
int fugirVoador = 1, voador = 0;//variavel para o final morrendo pro bicho voador
int semRosto = 0;//variavel para o final morrendo pro sem rosto
int caracol = 0;//variavel para o final morrendo pro caracol
int sorriso = 0; //variavel para o final morrendo para entidade da casa
int casa = 0; //sair morto ou vivo da casa 
void enter(){
    while (getchar() != '\n');
}


void limparTela() {
    printf("\033[H\033[J"); // Sequência de escape ANSI para limpar a tela
}

void pausa(){

   printf("Pressione Enter para continuar...");

    while (getchar() != '\n'); // Aguarda até que Enter seja pressionado
  sleep(2);
  limparTela();
}


void narrar(const char* mensagem) {
    printf("%s\n", mensagem);
    sleep(2); // Pausa por 2 segundos

}

void iniciarJogo(){
  narrar("26 de fevereiro de 1998\n");
  narrar("quinta feira, 6h14 da manhã\n");
  narrar("Os primeiros raios de sol tímidos começam a iluminar as ruas do bairro da Terra Firme.");
}

int validarResposta(int min, int max) {
    int escolha;

    while (1) {
        printf("Escolha uma opção (%d-%d): ", min, max);
        scanf("%d", &escolha);

        if (escolha >= min && escolha <= max) {
            return escolha;  // Retorna a escolha válida
        } else {
            printf("Opção inválida. Por favor, escolha entre %d e %d.\n", min, max);
        }
    }
}
void fugirSemrosto(){
  narrar("Continuando correndo, Você se pergunta o que era aquilo... ");
  pausa();
  narrar("Isso não é natural...");
  narrar("Mas é dificil pensar com toda essa adrenalina.");
  narrar("Você já deve ter se afastado o suficiente.");
  narrar("Parando de correr mas continuando andando");
  narrar("Você vira a esquina com a intenção de descansar em um local seguro do homem sem rosto...");
  balao = 1;
}
  
void sairSemrosto(){
  enter();
  pausa();
  narrar("Você resolve ir embora, mesmo sendo a única pessoa que você encontrou por aqui, mas algo nessa pessoa não parece certo...");
  narrar("Deve ser mais seguro ir embora...");
  narrar("Pelo menos agora, você não está completamente sozinho, e talvez possa encontrar alguém para pedir orientação. ");
  narrar("O ambiente estava ficando cada vez mais sinistro, mas você estava ansioso por um encontro amigável.");
  narrar("O que você estava pensando né? é claro que ta tudo bem... só precisa encontrar alguem que seja menos ameaçador...");
  narrar("E então, como se em resposta às suas preces, você vê alguém virando a esquina à frente. ");
  narrar(" Era um alívio ver outra pessoa ali; afinal, estava ficando muito sinistro naquele lugar. ");
  narrar(" Você caminha na direção da figura que se aproxima, esperando obter alguma ajuda ou informações.");

          balao = 1;
}

//função para quando o jogador resolver encostar na entidade sem rosto
void encostarSemRosto(){

                      narrar("Você se aproxima mais desse homem estranho");
                      narrar("Ele parece ja ter uma idade avançada...");
                      narrar("Ele está de costas para você, usando roupas de escritório.");
                      narrar("Mesmo se aproximando, o homem não parece ter notado sua presença");
                      narrar("Você sente um arrepio percorrer sua espinha, algo não parece certo...");
                      enter();
                      pausa();
                      narrar("Você se aproxima o suficiente dele, e põe sua mão em seu ombro");
                      narrar("Com a esperança que ele apenas não te ouviu, você diz:");
                      narrar("- Senhor? pode me ajudar?");
                      narrar("Você sente uma frieza ao encostar nele, para você, é como se estivesse tocando algo inanimado");

                      pausa();
                      narrar("Ele se vira lentamente a você...");
                      narrar("Quando ele vira o suficiente a ponto de você enxergar seu rosto...");
                      narrar("Uma sensação de pavor toma conta de você.");
                      narrar("Você nunca imaginaria ver isso, esse homem...");
                      narrar("Não tem rosto.");
                      pausa();
                      narrar("Ao invés das feições humanas normais,");
                      narrar("Você vê apenas uma superfície lisa aonde deveria ser seu rosto");
                      narrar("Como se alguém tivesse pegado uma borracha e apagado qualquer resquício de identidade desse homem.");
                      narrar("Ele vira seu 'rosto' em sua direção e...");
                      narrar("Agarra sua mão.");
                      narrar("Ele te segura com muita força.");
                      narrar("E para você, sem entender que criatura é essa, luta contra o medo para agir.");
                      pausa();
                      medo = 1;
                      narrar("O que fará diante dessa situação?");
                      printf("1. Se debater e sair correndo.\n2. Tentar o derrubar e sair correndo\n3. Gritar para que ele te solte\n");
                      escolha = validarResposta(1, 3);
                      if (escolha == 3){
                        narrar("Você está um desespero crescente enquanto o homem sem rosto aperta sua mão com força absurda.");
                        enter();
                        pausa();
                        narrar("Você grita o mais alto que pode, ecoando seu terror pela manhã tranquila.");
                        narrar("Seu grito corta o silêncio da manhã e ressoa como um lamento desesperado.");
                        narrar("Mas o homem sem rosto não se move, e sua aderência não afrouxa.");
                        narrar("Você começa a perder a sensação em sua mão, e a dor intensa se espalha por seu corpo.");
                       
                        pausa();
                        narrar("Não há mais chances para você, e você percebe isso...");
                        narrar("À medida que a escuridão envolve seus sentidos e sua visão se torna turva, ele se pergunta se há alguma esperança.");
                        narrar("Seu destino permanece incerto...");
                        narrar("o que acontecerá agora?");
                          semRosto = 1;
                          vida = 0;

                      } else {
                        if (escolha == 1){
                          narrar("Você se debate com todas as suas forças, conseguindo se soltar do aperto gélido do homem sem rosto.");
                          enter();
                          pausa();
                          narrar("Recuando rapidamente, respirando pesadamente e com os olhos ainda cheios de terror.");
                          narrar("O homem sem rosto não faz nenhum movimento para persegui-lo quando Afonso se afasta.");
                          narrar("Ele permanece de pé no mesmo lugar, sua figura sinistra iluminada pela luz fraca que penetra no beco.");
                          narrar("Você percebe que o estranho não tentou impedi-lo de fugir.");
                    
                          pausa();
                          fugirSemrosto();


    
                        } else{
                          narrar("Você decide lutar contra o homem sem rosto, seus músculos tensos e o coração batendo descontroladamente.");
                          narrar("Você o empurra com tudo que tem, tentando derrubar a criatura sinistra utilizando todo o seu conhecimento de judô que treinou quando era mais novo.");
                          enter();
                          pausa();
                          narrar("Há um breve momento de luta feroz, e finalmente, você consegue empurrar o homem sem rosto ao chão.");
                          narrar("O impacto faz com que a criatura solte sua mão, permitindo que se afaste rapidamente.");
                          narrar("A sua respiração está ofegante, você olha para trás para ter certeza de que o estranho não o está perseguindo.");
                          narrar("O homem sem rosto permanece no chão, imóvel, sua figura estranha se destacando na luz fraca da rua.");
                          narrar("Você sente um misto de alívio e terror, tentando entender o que acabou de acontecer.");
                          narrar("Decidindo não perder mais tempo ali, você começa a correr, afastando-se da cena macabra.");
                          narrar("A cada passo que dá, sua mente se enche de perguntas e medos, sem respostas claras à vista.");
                          
                          pausa();
                          fugirSemrosto();

                        }
                      }

}
//funçãop para encontrar o balao

void encontroBalao(){

    narrar("Quando vira a esquina escura e estreita, seu coração afundou ainda mais. Lá, à sua frente, estava uma figura grotesca.");
    narrar("Era uma criatura humanoide, mas sua cabeça, inflada como um balão gigante, era a característica mais perturbadora.");
    narrar("Afonso podia sentir a presença maligna pairando sobre ele. ");
    narrar("A criatura se movia de forma desengonçada, como se estivesse amarrada a essa cabeça inchada e desproporcional, balançando para a frente e para trás, quase flutuando no ar.");
    narrar("Os olhos da criatura, pequenos e opacos, fixaram-se em Afonso com uma expressão vazia e ameaçadora.");
    narrar("Afonso sentiu um arrepio percorrer sua espinha e um calafrio de horror profundo correr por suas veias.");


}
//perdercarro
void perderCarro(){

  narrar("Você decide voltar ao local onde estava estacionado seu carro...");        
  enter();
  pausa();
  narrar("procurando alguma segurança ou familiaridade em meio a essa situação bizarra.");  
  narrar("No entanto, sua confusão e desespero aumentam quando ele percebe que o carro desapareceu.");         
  narrar("Ele olha ao redor em busca de pistas, mas não encontra nenhum sinal do veículo.");
  narrar("A sensação de desamparo cresce à medida que ele se vê em um local vazio e desconhecido, sem entender como seu carro desapareceu sem que ele percebesse.");         
  narrar("Perguntas inundam sua mente, mas as respostas parecem distantes, e ele se sente perdido e desorientado.");
}
//função de um ramo da historia que acontece sem o carro
void semCarro(){
  printf("O que deseja fazer?\n1.Sair andando a procura de alguém.\n2. Gritar por ajuda aleatoriamente.\n3. Voltar por onde veio.\n");
    escolha = validarResposta(1, 3);
        switch (escolha){
          case 1:
            narrar("Você decide que a melhor opção é sair a pé em busca de alguma pista ou, pelo menos, da companhia de outra pessoa.");
            narrar("Enquanto caminha pelas ruas vazias, uma sensação de solidão e desamparo toma conta de você.");
            narrar(". Você tenta manter o ânimo, forçando um sorriso enquanto racionaliza que talvez todos os habitantes deste bairro tenham decidido viajar ao mesmo tempo.");
            narrar("É uma coincidência estranha, mas não há motivo para entrar em pânico, certo?");
            enter();
            pausa();
            narrar("A cada passo que dá, a cidade se revela cada vez mais estranha. ");
            narrar("Você não encontra nenhuma alma viva nas ruas e nenhum sinal de movimento nos prédios à sua volta.");
            narrar(" A sensação de que algo está muito errado só aumenta, mas você se recusa a admitir o medo.");
            narrar("Enquanto continua sua caminhada solitária, tentando manter-se são, você se pergunta o que pode estar acontecendo nesta cidade.");
            narrar("As dúvidas crescem, mas as explicações continuam a fugir.");
            narrar("Você sabe que precisa encontrar alguém, qualquer pessoa, para obter informações ou pelo menos alguma companhia para enfrentar essa estranha situação.");
            pausa();
            narrar("Mas...");
            narrar("Seu desamparo se transforma em um alívio instantâneamente ao avistar uma silhueta que parece ser de um homem adulto no meio da névoa andando.");
            narrar(" - EI!, Você pode me ajudar? - você diz");
            narrar("Mas a figura estranha não reage ao seu chamado");
            narrar("O homem apenas continua se afastando andando lentamente.");
            printf("O que deseja fazer?\n1. Tentar o chamar novamente.\n2. Se aproximar e encostar em seu ombro\n3. Ir embora\n");
            escolha = validarResposta(1, 3);
              if (escolha == 1){
                  enter();
                  narrar("Você o chama novamente.");
                  narrar("Mas não há resposta alguma.");
                  printf("O que deseja fazer?\n1.Se aproximar e encostar em seu ombro\n2. Ir embora\n");
                escolha = validarResposta(1, 2);
                      if (escolha == 1){
                        encostarSemRosto();
                        balao = 1;
                        medo = 1;
                        enter();
                        pausa();
                      } else{
                        sairSemrosto();
                        //ir embora e encontrar o balão
                        balao = 1;
                      }
              }  else if(escolha == 2){
                encostarSemRosto();
                medo = 1;
              } else {
               sairSemrosto();
                //encontrar balao
              }
          break;
          case 2:
            narrar("Você sai pelas ruas gritando, o pânico toma conta de você");
            narrar("Alguém pode ter roubado seu carro? Você não sabe responder se isso seria possível...");
            narrar("A estranheza de não ver ninguém nas rua começa a dominar seus pensamentos.");
            narrar("Você grita por ajuda, sua voz ecoa pelas ruas vazias, mas não há resposta, apenas o eco distante de suas próprias palavras.");
            narrar("O que está acontecendo? Por que a cidade está tão deserta?");
            narrar("As perguntas sem resposta continuam a martelar sua mente enquanto você continua sua busca desesperada por alguma presença, por alguma pista que possa explicar essa situação estranha.");
            pausa();
            narrar("Desamparado e com o coração pesado, você tenta se convencer de que não há nada demais nisso.");
            narrar("Afinal, deve ter sido apenas um erro ter entrado em uma rua desconhecida por acaso, certo? ");
            pausa();
            narrar("À medida que continua a caminhar, seus olhos captam uma silhueta na névoa, virando a esquina à sua frente. ");
            narrar("Um alívio quase palpável toma conta de você.");
            narrar("Finalmente, outra pessoa!");
            narrar("Você decide segui-la, esperando encontrar alguém que possa explicar o que está acontecendo ou dar alguma orientação.");
            pausa();
            narrar("Seu passo apressado o leva na direção da figura misteriosa.");
            narrar("Você chama por ela, tentando alcançá-la, mas a pessoa parece não ouvir ou ignorá-lo deliberadamente.");
            narrar("Ao virar a esquina...");
            balao = 1;
          break;
          case 3:
            narrar("Você decide voltar por onde veio.");
            narrar("A pé, você continua a retornar o caminho.");
            narrar("E continua não reconhecendo esse local, você está diante de prédios enorme e peculiares, você deveria se lembrar de ver isso, afinal, Você acabou de passar por aqui não foi?");
            narrar("Mas você só deve estar paranóico");
            narrar("É normal, você acabou de ter o carro assaltado e nem viu...");
            enter();
            pausa();
            narrar("O ambiente é solitário e silencioso");
            narrar("Os únicos barulhos do ambiente são os sons dos seus passos.");
            narrar("Você segue andando por longos minutos...");
            narrar("E nada parece mudar.");
            pausa();
            // Encontro com a entidade voadora
            narrar("Você continua andando, cada passo pesando cada vez mais em seus ombros.");
            narrar("A sensação de estranheza é contínua, e você se sente cada vez mais desamparado.");
            narrar("É isso? Você vai continuar perdido nesse local maldito?");
            narrar("Sua esperança começa a desaparecer, você não sabe aonde está nem por qual motivo isso está acontecendo com você.");
            narrar("...");
            pausa();
            narrar("Você se cansa de tanto andar, quanto tempo se passou?");
            narrar("Minutos?");
            narrar("Horas?");
            narrar("Dias??");
            narrar("Você não sabe mais...");
            narrar("É como se você não saísse do lugar...");
            narrar("e agora?");
            narrar("Hm? o que é isso? é estranho...");
            pausa();
            narrar("Um arrepio percorre sua espinha e você sente a necessidade de olhar para o céu.");
            narrar("Ao erguer os olhos, sua respiração fica presa em sua garganta.");
            narrar("Lá, flutuando no céu acima de você, uma massa amorfa e grotesca de carne e tentáculos se contorce e se move sem asas.");
            narrar("Seus muitos olhos encaram você, e um arrepio de terror absoluto corre por sua espinha.");
            narrar("Você nunca viu algo assim antes, algo que desafia toda a lógica e compreensão.");
            narrar("A criatura voadora continua a flutuar no ar, pairando sobre você como uma presença sinistra e indescritível.");
            narrar("O que é isso???");
            narrar("Que lugar é esse???");
            narrar("POR QUE ISSO ESTÁ ACONTECENDO COMIGO???");
            narrar("Muitos pensamentos inundam sua mente.");
            
            pausa();
            narrar("Você se sente totalmente impotente diante dessa abominação inominável.");
            narrar("O que você fará diante dessa visão aterradora?");
            printf("1. Ficar parado e observar a criatura.\n2. Correr em pânico.\n");
            escolha = validarResposta(1, 2);

              if (escolha == 1) {
      // Continuar observando a criatura
                fugirVoador = 0;
                narrar("Você decide ficar parado, hipnotizado pelo horror da criatura voadora.");
                enter();
                pausa();
                narrar("Cada movimento da criatura parece não seguir nenhuma lei natural, e seu medo é avassalador.");
                narrar("Enquanto você observa, a criatura começa a emitir um som estridente e inumano, como uma cacofonia de gritos distorcidos.");
                narrar("A pressão em sua mente parece insuportável, e você sente que sua sanidade está pendurada por um fio.");
             
                pausa();
                narrar("O que acontecerá agora?");
                vida = 0;
  } else if (escolha == 2){
      // Correr em pânico
      narrar("O pânico toma conta de você e você começa a correr desesperadamente em direção oposta à criatura voadora.");
                enter();
                pausa();
                
      narrar("Seus pés batem no asfalto enquanto você se afasta da terrível visão no céu.");
      narrar("A criatura emite um som ensurdecedor atrás de você, e você sente sua presença malévola perseguindo-o.");
      narrar("Você corre como nunca correu antes, com o medo impulsionando cada passo.");
                
                pausa();
        balao = 0;
  }

            //vaichegar na casa

          break;
        }

}
void Caracolmorte(){
  narrar("Marlene adentra silenciosamente a casa, sua Desert Eagle especial pronta para qualquer ameaça que possa surgir. ");
  narrar("Enquanto investiga os cômodos escuros e empoeirados, ela nota um rastro de gosma incomum no chão. Sem hesitar, ela decide seguir esse rastro viscoso e perturbador pelas escadas.");
  narrar("Ao chegar ao andar de cima, ela é confrontada com uma visão grotesca. ");
  pausa(); 
  narrar("Uma criatura de aparência asquerosa se estende diante dela. ");
  narrar("Parece ser um amálgama de vários corpos que se combinaram para criar algo que se assemelha a um caracol gosmento. "); 
  narrar("A criatura emite um som perturbador e parece tentar convencê-la a se juntar a eles, emitindo uma atração perturbadora.Marlene sente um estranho desejo de ceder à tentação, como se algo na criatura estivesse mexendo com sua mente. "); 
  narrar("No entanto, sua experiência como agente da ACE a alerta de que isso é apenas um truque da entidade, uma armadilha perigosa. "); 
  narrar("Ela puxa sua arma e começa a disparar, repetidamente, com uma determinação inabalável.A criatura é enorme e se contorce em desespero, tentando escapar dos tiros. ");
  narrar("Mas Marlene é mais rápida, e seus tiros precisos atingem a criatura repetidamente. "); 
  narrar("Com um último estrondo, a criatura se desfaz em uma névoa escura, dissipando-se no ar."); 
  narrar("Marlene observa atentamente o quarto, mas quem quer que estivesse ali, já partiu há muito tempo. "); 
  narrar("Ela sabe que seu dever é proteger o mundo dos horrores sobrenaturais, e, com sua mente e corpo ainda intactos, continua sua busca por respostas e ameaças que possam surgir em seu caminho.");
}void ACE(){   
     narrar("...");
    pausa();
    narrar("03 de março de 1998\n");
    narrar("4h29\n");

    narrar("A ACE, ou Agência de Contenção de Entidades, é uma organização especializada em proteger a população contra ameaças incomuns. "); 
    narrar("Muitas vezes, essas ameaças são o que as pessoas considerariam mitos ou histórias inventadas. No entanto, dentro da ACE, temos plena consciência de que essas criaturas e fenômenos são reais.");

    narrar("Nosso principal objetivo é manter a ilusão de que essas ameaças são apenas lendas, para que o público em geral não entre em pânico.");
    narrar("Para atingir esse objetivo, agimos de maneira eficaz e discreta. Investigamos casos envolvendo o sobrenatural e tomamos medidas para contê-los o mais rapidamente possível."); 
    narrar("Isso não apenas protege as pessoas de possíveis danos, mas também evita que o conhecimento sobre o mundo paranormal se espalhe. Acreditamos que é crucial manter um equilíbrio entre o mundo normal e o sobrenatural, para o bem de todos.");
    pausa();
    narrar("O local em que Afonso se perdeu é um lugar assombrado, uma região que se tornou infestada por atividades paranormais devido à presença de uma criatura paranormal incrivelmente poderosa. "); 
    narrar("Essas áreas assombradas tendem a atrair outras criaturas e enfraquecem a barreira que separa o mundo normal do mundo espiritual.");

    narrar("A ACE tem estado monitorando essa situação de perto e investigando o caso. Vários relatos de pessoas desaparecidas na área aumentaram a preocupação, e Afonso é uma das vítimas desse desaparecimento."); 
    narrar("Hoje, a equipe da ACE entrou em uma missão de resgate, determinada a encontrar Afonso e qualquer outra pessoa que possa estar presa nessa região assombrada. Agora, eles estão prestes a enfrentar os desafios dessa missão perigosa.");

    narrar("Seu nome é Marlene, uma agente da ace, e está no meio de uma missão de resgate."); 
    narrar("A investigação da agência indica que diversas pessoas desapareceram em circunstâncias misteriosas nesse local assombrado. Agora, sua equipe adentra nesse local amaldiçoado, determinada a trazer de volta os desaparecidos. ");
    pausa();
    narrar(" Ao entrar nesse local desconhecido, você nota logo de cara a neblina densa que paira sobre a área. ");
    narrar("Ela envolve as ruas estreitas, obscurecendo sua visão e criando uma atmosfera enigmática. Cada passo que você dá parece ecoar, abafado pelo manto branco que envolve tudo.");
    narrar("Você continua a adentrar as ruas, guiado apenas pelo fraco brilho dos postes de luz.");
    narrar("O ambiente ao seu redor parece deserto, com silhuetas de edifícios antigos que se erguem nas sombras. Não há sinal de vida ou movimento, exceto por sua própria presença solitária");
    narrar("Mas você sabe que aqui não é seguro. Você não é uma simples civil; está fortemente armada e treinada para lidar com situações perigosas."); 
    narrar("Com determinação, você decide procurar em cada canto dessa cidade, na esperança de encontrar qualquer vítima viva que possa estar escondida nas sombras.");
    narrar("Com a sua lanterna tática em mãos e a arma pronta, você começa a explorar becos estreitos, praças silenciosas e ruas desertas."); 
    narrar("A névoa densa não desanima você; na verdade, ela aumenta ainda mais a sensação de que algo terrível aconteceu aqui. Cada sombra parece esconder um segredo, e você está determinado a desvendá-los.");
    pausa();
    narrar("À medida que avança pela cidade, seus sentidos aguçados permanecem alertas. "); 
